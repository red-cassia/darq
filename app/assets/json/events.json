        }
      ]
    },
    "query": "query { items: events { id, display_name, display_picture, street_address type description  hours{ open close all_day }   }  }"
  },
  "detailed": {
    "appbar": {
      "filter": false,
      "title": "events"
    },
    "columns": {
      "header": {
        "start": [
          {
            "widget": "profile_picture_detailed_page",
            "data": ["display_picture"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "contact_button_detailed_page",
            "titleText": "contact_us"
          }
        ],
        "end": [
          {
            "widget": "display_name_detailed_page",
            "data": ["display_name"]
          },
          {
            "widget": "basic_text_detailed_page",
            "data": ["type"]
          },
          {
            "widget": "vertical_spacer",
            "height": 3.0
          },
          {
            "widget": "text_with_icon",
            "iconName": "address.png",
            "joinedData": {
              "separator": ", ",
              "children": [
                ["street_address"],
                ["city"]
              ]
            }
          },
          {
            "widget": "vertical_spacer",
            "height": 2.0
          },
          {
            "widget": "operating_hours",
            "data": ["hours"]
          },
          {
            "widget": "vertical_spacer",
            "height": 2.0
          },
          {
            "widget": "phone_number",
            "iconName": "telephone.png",
            "maxElements": 2,
            "data": ["phone_number"]
          }
        ]
      },
      "body": [
        {
          "widget": "vertical_spacer",
          "height": 2.0
        },
        {
          "widget": "text",
          "textSize": "title9",
          "textColor": {"r": 13, "g": 36, "b": 52, "o": 1.0 },
          "data": ["description"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "duration",
          "data": ["duration"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "text_with_title",
          "titleText": "ticket_reservation",
          "titleSize": "title9",
          "titleColor": { "r": 13, "g": 36, "b": 52, "o": 1.0 },
          "titleFontWeight": "w700",
          "textSize": "text9odd",
          "textColor": { "r": 9, "g": 93, "b": 106, "o": 1.0 },
          "data": ["ticket_website"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "price",
          "titleText": "ticket_price",
          "titleSize": "title9",
          "titleColor": { "r": 13, "g": 36, "b": 52, "o": 1.0 },
          "titleFontWeight": "w700",
          "textSize": "title10",
          "textColor": { "r": 0, "g": 0, "b": 0, "o": 0.7 },
          "trailingText": "",
          "trailingTextSize": "title10",
          "trailingTextColor": { "r": 9, "g": 93, "b": 106, "o": 1.0 },
          "data": ["ticket_price"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "text_with_title",
          "titleText": "organizer",
          "titleSize": "title9",
          "titleColor": { "r": 13, "g": 36, "b": 52, "o": 1.0 },
          "titleFontWeight": "w700",
          "textSize": "text9odd",
          "textColor": { "r": 9, "g": 93, "b": 106, "o": 1.0 },
          "data": ["organizer"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "picture_gallery",
          "titleText": "photos",
          "titleSize": "title9",
          "titleColor": { "r": 13, "g": 36, "b": 52, "o": 1.0 },
          "titleFontWeight": "w700",
          "data": ["attachments"]
        }
      ]
    },
    "query": "query($id: ID!) { item: event(id: $id) { display_name display_picture type street_address hours{ open close all_day } city phone_number description duration{ start end } ticket_website ticket_price{ value valueLower valueUpper currency } organizer attachments } }"
  }
}
