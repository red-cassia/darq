    "query": "query($id: ID!) {\n  item: event(id: $id) {\n    display_name\n    display_picture\n    type\n    street_address\n    hours{\n      open\n      close\n      all_day\n    }\n    phone_number\n    description\n    duration{\n      start\n      end\n    }\n    ticket_website\n    ticket_price{\n      value\n      valueLower\n      valueUpper\n      currency\n    }\n    organizer\n    attachments\n  }\n}"
