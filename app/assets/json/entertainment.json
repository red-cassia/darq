{
  "list": {
    "appbar": {
      "filter": true,
      "title": "entertainment",
      "values_query": "query { __type(name: \"EntertainmentBusinessSubType\") { enumValues { name } } }"
    },
    "columns": {
      "start": [
        {
          "widget": "profile_picture_list_page",
          "data": ["display_picture"]
        },
        {
          "widget": "vertical_spacer",
          "height": 7.0
        },
        {
          "widget": "rating",
          "data": ["rating"]
        }
      ],
      "end": [
        {
          "widget": "text",
          "textSize": "title9",
          "textColor": {"r": 13, "g": 36, "b": 52, "o": 1.0},
          "fontWeight": "w700",
          "data": ["display_name"]
        },
        {
          "widget": "text",
          "textSize": "title9Odd",
          "textColor": {"r": 9, "g": 93, "b": 106, "o": 1.0 },
          "data": ["sub_type_string"]
        },
        {
          "widget": "vertical_spacer",
          "height": 3.0
        },
        {
          "widget": "text_overflowed",
          "textSize": "text9",
          "textColor": { "r": 0, "g": 0, "b": 0, "o": 0.37 },
          "data": ["description"]
        },
        {
          "widget": "vertical_spacer",
          "height": 2.0
        },
        {
          "widget": "view_details_button",
          "data": ["id"]
        },
        {
          "widget": "vertical_spacer",
          "height": 10.0
        },
        {
          "widget": "contact_button_list_page",
          "titleText": "contact_us",
          "data": ["id"]
        }
      ]
    },
    "query": "query ($sub_types: [String]) { items: businesses(type: EntertainmentBusiness, sub_types: $sub_types) { id, display_name, display_picture, rating ... on EntertainmentBusiness { sub_type_string description } }  }"
  },
  "detailed": {
    "appbar": {
      "filter": false,
      "title": "entertainment"
    },
    "columns": {
      "header": {
        "start": [
          {
            "widget": "profile_picture_detailed_page",
            "data": ["display_picture"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "rating",
            "data": ["rating"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "contact_button_detailed_page",
            "titleText": "contact_us"
          }
        ],
        "end": [
          {
            "widget": "display_name_detailed_page",
            "data": ["display_name"]
          },
          {
            "widget": "basic_text_detailed_page",
            "data": ["sub_type_string"]
          },
          {
            "widget": "vertical_spacer",
            "height": 3.0
          },
          {
            "widget": "text_with_icon",
            "iconName": "address.png",
            "joinedData": {
              "separator": ", ",
              "children": [
                ["street_address"],
                ["city"]
              ]
            }
          },
          {
            "widget": "vertical_spacer",
            "height": 2.0
          },
          {
            "widget": "operating_hours",
            "data": ["operating_hours"]
          },
          {
            "widget": "vertical_spacer",
            "height": 2.0
          },
          {
            "widget": "phone_number",
            "iconName": "telephone.png",
            "maxElements": 2,
            "data": ["phone_number"]
          }
        ]
      },
      "body": [
        {
          "widget": "vertical_spacer",
          "height": 9.0
        },
        {
          "widget": "text",
          "textSize": "text8w500",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.69 },
          "data": ["description"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "itemized_text_with_title",
          "titleText": "activities",
          "titleSize": "title9",
          "titleColor": { "r": 0, "g": 0, "b": 0, "o": 0.67 },
          "textSize": "text9odd",
          "textColor": { "r": 0, "g": 0, "b": 0, "o": 0.5 },
          "data": ["activities"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "website_with_title",
          "titleText": "website",
          "titleSize": "title9",
          "titleColor": { "r": 0, "g": 0, "b": 0, "o": 0.67 },
          "textSize": "text9odd",
          "textColor": { "r": 66, "g": 102, "b": 118, "o": 1.0 },
          "data": ["website"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "picture_gallery",
          "titleText": "photos",
          "titleSize": "title9",
          "titleColor": { "r": 0, "g": 0, "b": 0, "o": 0.67 },
          "data": ["attachments"]
        }
      ]
    },
    "query": "query($id: ID!) { item: business(id: $id) { display_name display_picture rating city ... on EntertainmentBusiness { sub_type_string street_address city operating_hours { open close all_day } phone_number description website attachments activities } } } "
  }
}
