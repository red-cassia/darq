{
  "list": {
    "appbar": {
      "filter": false,
      "title": "Domestic Help"
    },
    "columns": {
      "start": [
        {
          "widget": "picture",
          "height": 69.0,
          "width": 70.0,
          "data": ["display_picture"]
        },
        {
          "widget": "vertical_spacer",
          "height": 7.0
        },
        {
          "widget": "rating",
          "data": ["rating"]
        }
      ],
      "end": [
        {
          "widget": "text",
          "textSize": "title9",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.7 },
          "data": ["display_name"]
        },
        {
          "widget": "text",
          "textSize": "title11Odd",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
          "data": ["sub_type_string"]
        },
        {
          "widget": "vertical_spacer",
          "height": 3.0
        },
        {
          "widget": "text_overflowed",
          "textSize": "text10w500",
          "textColor": { "r": 0, "g": 0, "b": 0, "o": 0.37 },
          "data": ["description"]
        },
        {
          "widget": "vertical_spacer",
          "height": 2.0
        },
        {
          "widget": "view_details_button",
          "data": ["id"]
        },
        {
          "widget": "vertical_spacer",
          "height": 10.0
        },
        {
          "widget": "contact_button",
          "titleText": "Contact Us",
          "data": ["id"]
        }
      ]
    },
    "query": "query ($sub_types: [String]) {\n  items: businesses(type: DomesticHelpBusiness, sub_types: $sub_types) {\n    id,\n    display_name,\n    display_picture,\n    rating\n    ... on DomesticHelpBusiness {\n      sub_type_string\n      description\n    }\n  } \n}"
  },

  "detailed": {
    "appbar": {
      "filter": true,
      "text": "Contact Us",
      "values_query": "query {\n    __type(name: \"Profession\") {\n      enumValues {\n        name\n      }\n    }\n}  ",
      "filter_data_path": "personnel",
      "filter_attribute": "profession"
    },
    "columns": {
      "header": {
        "start": [
          {
            "widget": "picture",
            "height": 79.0,
            "width": 80.0,
            "data": ["display_picture"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "rating",
            "data": ["rating"]
          }
        ],
        "end": [
          {
            "widget": "text",
            "textSize": "title9",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.7 },
            "data": ["display_name"]
          },
          {
            "widget": "text",
            "textSize": "title11Odd",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
            "data": ["sub_type_string"]
          },
          {
            "widget": "vertical_spacer",
            "height": 4.0
          },
          {
            "widget": "text_with_icon",
            "textSize": "title11Odd",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
            "iconName": "address.png",
            "data": ["street_address"]
          },
          {
            "widget": "vertical_spacer",
            "height": 5.0
          },
          {
            "widget": "wrapped_text_with_icon",
            "textSize": "title10OddUnderlined",
            "textColor": {"r": 66, "g": 102, "b": 119, "o": 1.0 },
            "iconName": "telephone.png",
            "maxElements": 2,
            "data": ["phone_number"]
          }
        ]
      },
      "body": [
        {
          "widget": "vertical_spacer",
          "height": 9.0
        },
        {
          "widget": "text",
          "textSize": "text6w500",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.69 },
          "data": ["description"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "personnel",
          "data": ["personnel"]
        }
      ]
    },
    "query": "query($id: ID!) {\n  item: business(id: $id) {\n    display_name\n    display_picture\n    rating \n  \n    ... on DomesticHelpBusiness {\n      sub_type_string\n      street_address\n      phone_number\n      description\n      personnel{\n        name\n        picture\n        name\n        gender\n        nationality\n        religion\n        profession\n        salary{\n          value\n          valueLower\n          value\n          currency\n        }\n        date_of_birth\n        marital_status\n        languages\n        experience{\n          country\n          institution\n          country\n          from\n          to\n          in_position\n        }\n        phone_number\n        ... on Nanny {\n             education\n             height\n             weight\n             skills\n             number_of_children\n        }\n        ... on Maid {\n             education\n             height\n             weight\n             skills\n             number_of_children\n        }\n        ... on Cook {\n             education\n             height\n             weight\n             skills\n             number_of_children\n        }\n        ... on Driver {\n        license_expiry_date\n        }\n      }\n    }\n  }\n}"
  }
}
