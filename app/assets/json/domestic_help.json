{
  "list": {
    "appbar": {
      "filter": false,
      "title": "domestic_help"
    },
    "columns": {
      "start": [
        {
          "widget": "picture",
          "height": 69.0,
          "width": 70.0,
          "data": ["display_picture"]
        },
        {
          "widget": "vertical_spacer",
          "height": 7.0
        },
        {
          "widget": "rating",
          "data": ["rating"]
        }
      ],
      "end": [
        {
          "widget": "text",
          "textSize": "title9",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.7 },
          "data": ["display_name"]
        },
        {
          "widget": "text",
          "textSize": "title11Odd",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
          "data": ["sub_type_string"]
        },
        {
          "widget": "vertical_spacer",
          "height": 3.0
        },
        {
          "widget": "text_overflowed",
          "textSize": "text10w500",
          "textColor": { "r": 0, "g": 0, "b": 0, "o": 0.37 },
          "data": ["description"]
        },
        {
          "widget": "vertical_spacer",
          "height": 2.0
        },
        {
          "widget": "view_details_button",
          "data": ["id"]
        },
        {
          "widget": "vertical_spacer",
          "height": 10.0
        },
        {
          "widget": "contact_button",
          "titleText": "contact_us",
          "data": ["id"]
        }
      ]
    },
    "query": "query ($sub_types: [String]) { items: businesses(type: DomesticHelpBusiness, sub_types: $sub_types) { id, display_name, display_picture, rating ... on DomesticHelpBusiness { sub_type_string description } }  }"
  },

  "detailed": {
    "appbar": {
      "filter": true,
      "text": "contact_us",
      "values_query": "query { __type(name: \"Profession\") { enumValues { name } } }  ",
      "filter_data_path": "personnel",
      "filter_attribute": "profession"
    },
    "columns": {
      "header": {
        "start": [
          {
            "widget": "picture",
            "height": 79.0,
            "width": 80.0,
            "data": ["display_picture"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "rating",
            "data": ["rating"]
          }
        ],
        "end": [
          {
            "widget": "text",
            "textSize": "title9",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.7 },
            "data": ["display_name"]
          },
          {
            "widget": "text",
            "textSize": "title11Odd",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
            "data": ["sub_type_string"]
          },
          {
            "widget": "vertical_spacer",
            "height": 3.0
          },
          {
            "widget": "text_with_icon",
            "textSize": "title11Odd",
            "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.5 },
            "iconName": "address.png",
            "joinedData": {
              "separator": ", ",
              "children": [
                ["street_address"],
                ["city"]
              ]
            }
          },
          {
            "widget": "vertical_spacer",
            "height": 2.0
          },
          {
            "widget": "wrapped_text_with_icon",
            "textSize": "title10OddUnderlined",
            "textColor": {"r": 66, "g": 102, "b": 119, "o": 1.0 },
            "iconName": "telephone.png",
            "maxElements": 2,
            "data": ["phone_number"]
          }
        ]
      },
      "body": [
        {
          "widget": "vertical_spacer",
          "height": 9.0
        },
        {
          "widget": "text",
          "textSize": "text8w500",
          "textColor": {"r": 0, "g": 0, "b": 0, "o": 0.69 },
          "data": ["description"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "website_with_title",
          "titleText": "website",
          "titleSize": "title9",
          "titleColor": { "r": 0, "g": 0, "b": 0, "o": 0.67 },
          "textSize": "text9odd",
          "textColor": { "r": 66, "g": 102, "b": 118, "o": 1.0 },
          "data": ["website"]
        },
        {
          "widget": "divider"
        },
        {
          "widget": "personnel",
          "data": ["personnel"]
        }
      ]
    },
    "query": "query($id: ID!) { item: business(id: $id) { display_name display_picture rating   ... on DomesticHelpBusiness { sub_type_string street_address phone_number description city website personnel{ name picture name gender nationality religion profession salary{ value valueLower value currency } date_of_birth marital_status languages attachments experience{ country institution from to in_position } phone_number ... on Nanny { education height weight skills number_of_children } ... on Maid { education height weight skills number_of_children } ... on Cook { education height weight skills number_of_children } ... on Driver { license_expiry_date } } } } }  "
  }
}
