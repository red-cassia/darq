        {
          "widget": "rate_us",
          "titleText": "rate_this_business",
          "data": ["rating"]
        }
      ]
    },
    "query": "query($id: ID!) { item: business(id: $id) { display_name display_picture rating ... on SelfEmployedBusiness { sub_type_string gender nationality phone_number description skills city experience { institution country from to in_position } charge{ value valueLower valueUpper currency } attachments } } }"
