        {
          "widget": "text_overflowed",
          "textSize": "title9",
          "textColor": {"r": 13, "g": 36, "b": 52, "o": 1.0 },
          "data": ["description"]
        },
        {
          "widget": "vertical_spacer",
          "height": 2.0
        },
        {
          "widget": "view_details_button",
          "data": ["id"]
        },
        {
          "widget": "vertical_spacer",
          "height": 10.0
        },
        {
          "widget": "contact_button_list_page",
          "titleText": "contact_me",
          "data": ["id"]
        }
      ]
    },
    "query": "query ($sub_types: [String]) { items: businesses(type: SelfEmployedBusiness, sub_types: $sub_types) { id, display_name, display_picture, rating ... on SelfEmployedBusiness { sub_type_string description } }  }"
  },
