    "query": "query { items: businesses(type: StationeryBusiness) { id, display_name, display_picture, rating ... on StationeryBusiness { city description } }  } "
  },
  "detailed": {
    "appbar": {
      "filter": false,
      "title": "stationeries"
    },
    "columns": {
      "header": {
        "start": [
          {
            "widget": "profile_picture_detailed_page",
            "data": ["display_picture"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "rating",
            "data": ["rating"]
          },
          {
            "widget": "vertical_spacer",
            "height": 6.0
          },
          {
            "widget": "contact_button_detailed_page",
            "titleText": "contact_us"
          }
        ],
        "end": [
          {
            "widget": "display_name_detailed_page",
            "data": ["display_name"]
          },
          {
            "widget": "basic_text_detailed_page",
            "text": "Stationery"
          },
          {
            "widget": "vertical_spacer",
            "height": 3.0
          },
          {
            "widget": "text_with_icon",
